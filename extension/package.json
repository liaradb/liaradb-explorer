{
  "name": "liaradb-explorer",
  "displayName": "liaradb-explorer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "liaradb-explorer.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "catCoding.start",
        "title": "catCoding start"
      },
      {
        "command": "catCoding.doRefactor",
        "title": "catCoding doRefactor"
      },
      {
        "command": "nodeDependencies.refreshEntry",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "nodeDependencies.editEntry",
        "title": "Edit",
        "icon": "$(refresh)"
      },
      {
        "command": "nodeDependencies.deleteEntry",
        "title": "Delete",
        "icon": "$(refresh)"
      },
      {
        "command": "serverTree.show",
        "title": "Show",
        "icon": "$(refresh)"
      },
      {
        "command": "serverTree.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "serverTree.addServer",
        "title": "Add Server",
        "icon": "$(plus)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-explorer",
          "title": "Package Explorer",
          "icon": "$(refresh)"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "nodeDependencies",
          "name": "Node Dependencies",
          "icon": "$(refresh)"
        },
        {
          "id": "serverTree",
          "name": "Server tree",
          "icon": "$(refresh)"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "nodeDependencies.refreshEntry",
          "when": "view == nodeDependencies",
          "group": "navigation"
        },
        {
          "command": "serverTree.refresh",
          "when": "view == serverTree",
          "group": "navigation"
        },
        {
          "command": "serverTree.addServer",
          "when": "view == serverTree",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "nodeDependencies.editEntry",
          "when": "view == nodeDependencies && viewItem == dependency",
          "group": "inline"
        },
        {
          "command": "nodeDependencies.deleteEntry",
          "when": "view == nodeDependencies && viewItem == dependency"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "nodeDependencies",
        "contents": "No node dependencies found [learn more](https://www.npmjs.com/).\n[Add Dependency](command:nodeDependencies.addEntry)"
      },
      {
        "view": "serverTree",
        "contents": "No servers found.\n[Add Server](command:serverTree.addServer)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "definitions_ts": "../definitions_ts",
    "@grpc/grpc-js": "^1.14.3",
    "@types/google-protobuf": "^3.15.12",
    "google-protobuf": "^4.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
